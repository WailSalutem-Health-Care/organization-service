# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=organization_db

# Keycloak Configuration
KEYCLOAK_BASE_URL=http://localhost:8180
KEYCLOAK_REALM=wailsalutem
KEYCLOAK_ADMIN_CLIENT_ID=admin-cli
KEYCLOAK_ADMIN_CLIENT_SECRET=your-client-secret

# RabbitMQ Configuration
RABBITMQ_URL=amqp://admin:admin123@localhost:5672/

# OpenTelemetry Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=localhost:4317
OTEL_SERVICE_NAME=organization-service
OTEL_SERVICE_NAMESPACE=wailsalutem
OTEL_SERVICE_VERSION=1.0.0
ENVIRONMENT=production
OTEL_TRACES_SAMPLER=always_on
OTEL_METRICS_EXPORT_INTERVAL=30s

# Application Configuration
PORT=8080
TZ=CET
